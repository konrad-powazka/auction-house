<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApplication3</title>

    <script src="~/dependencies/scripts/Rx.umd.js"></script>
    <script src="~/dependencies/scripts/angular/core.umd.js"></script>
    <script src="~/dependencies/scripts/angular/common.umd.js"></script>
    <script src="~/dependencies/scripts/angular/compiler.umd.js"></script>
    <script src="~/dependencies/scripts/angular/platform-browser.umd.js"></script>
    <script src="~/dependencies/scripts/angular/platform-browser-dynamic.umd.js"></script>
    <script asp-src-include="~/scripts/app/**/*.js"></script>

    <link rel="stylesheet" href="~/dependencies/styles/bootstrap.css" @*asp-append-version="true"*@ />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-controller="Home" asp-action="Index" class="navbar-brand">WebApplication</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-controller="Home" asp-action="About">About</a></li>
                    <li><a asp-controller="Home" asp-action="Contact">Contact</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        Test
        <hr />
        <footer>
            <p>WebApplication</p>
        </footer>
    </div>
</body>
</html>

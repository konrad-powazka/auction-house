{
  "compileOnSave": true,
  "compilerOptions": {
    //"out": "All.ts",
    "module": "none",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  },
  "include": [
    "**/*",
    "../typings/**/*"
  ]
}